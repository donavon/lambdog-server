{
  "name": "@lambdog/server",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "version": "0.1.0-alpha.1",
  "description": "It's part lamb. It's part dog. It's lambdog.",
  "module": "module/indedx.js",
  "source": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/donavon/lambdog-server.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "test": "jest --verbose --coverage --silent",
    "test:watch": "jest --watch --runInBand --silent",
    "prebuild": "npm run lint && npm t && rimraf module",
    "build": "babel ./src --out-dir ./module"
  },
  "keywords": [
    "utils",
    "lib",
    "lambda",
    "aws",
    "netlify",
    "functions"
  ],
  "author": "Donavon West <github@donavon.com> (https://github.com/donavon)",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-jest": "^24.7.1",
    "eslint": "^5.10.0",
    "eslint-config-amex": "^9.0.0",
    "jest": "^24.7.1",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {}
}
